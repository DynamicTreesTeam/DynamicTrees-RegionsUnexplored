{
  "multipart": [
    {
      "apply": {
        "model": "dtru:block/palm_leaves/joshua_frond"
      },
      "when": {
        "distance": "1|2"
      }
    },
    {
      "apply": {
        "model": "dtru:block/palm_leaves/joshua_core_top"
      },
      "when": {
        "distance": "3"
      }
    },
    {
      "apply": {
        "model": "dtru:block/palm_leaves/joshua_core_bottom"
      },
      "when": {
        "distance": "4"
      }
    },
    {
      "apply": {
        "model": "regions_unexplored:block/joshua_leaves_top"
      },
      "when": {
        "OR": [
          {
            "direction": "0",
            "distance": "1|2"
          },
          {
            "distance": "5|6|7"
          }
        ]
      }
    }
  ]
}